#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/admin/patrol-report/server/node_modules/.pnpm/json2csv@6.0.0-alpha.2/node_modules/json2csv/bin/node_modules:/home/admin/patrol-report/server/node_modules/.pnpm/json2csv@6.0.0-alpha.2/node_modules/json2csv/node_modules:/home/admin/patrol-report/server/node_modules/.pnpm/json2csv@6.0.0-alpha.2/node_modules:/home/admin/patrol-report/server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/admin/patrol-report/server/node_modules/.pnpm/json2csv@6.0.0-alpha.2/node_modules/json2csv/bin/node_modules:/home/admin/patrol-report/server/node_modules/.pnpm/json2csv@6.0.0-alpha.2/node_modules/json2csv/node_modules:/home/admin/patrol-report/server/node_modules/.pnpm/json2csv@6.0.0-alpha.2/node_modules:/home/admin/patrol-report/server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/json2csv@6.0.0-alpha.2/node_modules/json2csv/bin/json2csv.js" "$@"
else
  exec node  "$basedir/../.pnpm/json2csv@6.0.0-alpha.2/node_modules/json2csv/bin/json2csv.js" "$@"
fi
